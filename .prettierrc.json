{
    "plugins": ["prettier-plugin-go-template"],
    "overrides": [
        {
            "files": ["*.html"],
            "options": {
                "parser": "go-template"
            }
        }
    ],
    "goTemplateBracketSpacing": true,
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "none"
}
